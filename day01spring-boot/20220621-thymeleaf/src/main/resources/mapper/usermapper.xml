<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--设置映射的mapper-->
<mapper namespace="com.blb.thymeleaf.mapper.UserMapper">

    <select id="getByName" resultType="com.blb.thymeleaf.entity.User">
        select * from s_user where username =#{name}
    </select>
</mapper>